    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-graphql": "^0.9.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-import-node": "^0.0.4",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tools": "^7.0.5",
    "http": "*",
    "ncp": "^2.0.0",
    "typescript": "^4.3.2"
